{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://stayfinder-dewy.onrender.com/api/:path*"
    },
    {
      "source": "/cancellation-and-refund",
      "destination": "/cancellation-and-refund.html"
    },
    {
      "source": "/terms-and-conditions",
      "destination": "/terms-and-conditions.html"
    },
    {
      "source": "/shipping",
      "destination": "/shipping.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/contact-us",
      "destination": "/contact-us.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
